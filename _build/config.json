      "value": ""
    }, {
      "key": "user_email",
      "type": "textfield",
      "area": "default",
      "value": ""
    }]
  },
  "database": {},
  "build":{
    "resolver": {
      "after": ["setupoptions.resolver.php", "stercextra.resolver.php"]
    },
    "setupOptions":{
      "source": "setup.options.php"
    }
