{
  "name": "CallToActionTV",
  "lowCaseName": "calltoactiontv",
  "description": "Call To Action TV package.",
  "author": "Sterc",
  "version": "1.0.5",
  "package":{
    "menus": [],
    "elements": {
      "categories": [],
      "snippets": [{
        "name": "calltoactiontv",
        "file": "calltoactiontv.snippet.php"
      }],
      "chunks": [{
        "name": "callToActionTV",
        "file": "calltoactiontv.chunk.tpl"
      }],
      "plugins": [{
        "name": "CallToActionTV",
        "description": "",
        "file": "calltoactiontv.plugin.php",
